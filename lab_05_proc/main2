int pagemap_get_entry(PagemapEntry *entry, int pagemap_fd, uintptr_t vaddr)
{
    size_t nread;
    ssize_t ret;
    uint64_t data;

    nread = 0;
    while (nread < sizeof(data)) {
        ret = pread(pagemap_fd, ((uint8_t*)&data) + nread, sizeof(data) - nread,
                (vaddr / sysconf(
# 242 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c" 3 4
                                _SC_PAGESIZE
# 242 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c"
                                             )) * sizeof(data) + nread);
        nread += ret;
        if (ret <= 0) {
            return 1;
        }
    }
    entry->pfn = data & (((uint64_t)1 << 55) - 1);
    entry->soft_dirty = (data >> 55) & 1;
    entry->file_page = (data >> 61) & 1;
    entry->swapped = (data >> 62) & 1;
    entry->present = (data >> 63) & 1;
    return 0;
}

void printPAGEMAP()
{
    char buffer[
# 258 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c" 3 4
               8192
# 258 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c"
                     ];
    char maps_file[
# 259 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c" 3 4
                  8192
# 259 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c"
                        ];
    char pagemap_file[
# 260 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c" 3 4
                     8192
# 260 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c"
                           ];
    int maps_fd;
    int offset = 0;
    int pagemap_fd;
    pid_t pid;

    fprintf(f, "addr = Если страница присутствует в ОЗУ (бит 63), то эти биты обеспечивают номер страничного фрейма, Если страница присутствует в свопе (бит 62), затем биты 4–0 задают тип подкачки, а биты 54–5 кодируют смещение подкачки.\npfn \nsoft-dirty \nfile/shared = Страница представляет собой страницу с отображением файлов или общую анонимную страницу \nswapped = Если установлено, страница находится в пространстве подкачк \npresent = Если установлено, страница находится в оперативной памяти.\n\n");

    snprintf(maps_file, sizeof(maps_file), "/proc/%ju/maps", (uintmax_t)9014);
    snprintf(pagemap_file, sizeof(pagemap_file), "/proc/%ju/pagemap", (uintmax_t)9014);
    maps_fd = open(maps_file, 
# 270 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c" 3 4
                             00
# 270 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c"
                                     );
    if (maps_fd < 0) {
        perror("open maps");

    }
    pagemap_fd = open(pagemap_file, 
# 275 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c" 3 4
                                   00
# 275 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c"
                                           );
    if (pagemap_fd < 0) {
        perror("open pagemap");

    }
    fprintf(f, "addr pfn soft-dirty file/shared swapped present library\n");
    for (;;) {
        ssize_t length = read(maps_fd, buffer + offset, sizeof buffer - offset);
        if (length <= 0) break;
        length += offset;
        for (size_t i = offset; i < (size_t)length; i++) {
            uintptr_t low = 0, high = 0;
            if (buffer[i] == '\n' && i) {
                const char *lib_name;
                size_t y;
                {
                    size_t x = i - 1;
                    while (x && buffer[x] != '\n') x--;
                    if (buffer[x] == '\n') x++;
                    while (buffer[x] != '-' && x < sizeof buffer) {
                        char c = buffer[x++];
                        low *= 16;
                        if (c >= '0' && c <= '9') {
                            low += c - '0';
                        } else if (c >= 'a' && c <= 'f') {
                            low += c - 'a' + 10;
                        } else {
                            break;
                        }
                    }
                    while (buffer[x] != '-' && x < sizeof buffer) x++;
                    if (buffer[x] == '-') x++;
                    while (buffer[x] != ' ' && x < sizeof buffer) {
                        char c = buffer[x++];
                        high *= 16;
                        if (c >= '0' && c <= '9') {
                            high += c - '0';
                        } else if (c >= 'a' && c <= 'f') {
                            high += c - 'a' + 10;
                        } else {
                            break;
                        }
                    }
                    lib_name = 0;
                    for (int field = 0; field < 4; field++) {
                        x++;
                        while(buffer[x] != ' ' && x < sizeof buffer) x++;
                    }
                    while (buffer[x] == ' ' && x < sizeof buffer) x++;
                    y = x;
                    while (buffer[y] != '\n' && y < sizeof buffer) y++;
                    buffer[y] = 0;
                    lib_name = buffer + x;
                }

                {
                    PagemapEntry entry;
                    for (uintptr_t addr = low; addr < high; addr += sysconf(
# 332 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c" 3 4
                                                                           _SC_PAGESIZE
# 332 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c"
                                                                                        )) {

                        if (!pagemap_get_entry(&entry, pagemap_fd, addr)) {
                            fprintf(f, "%jx %jx %u %u %u %u %s\n",
                                (uintmax_t)addr,
                                (uintmax_t)entry.pfn,
                                entry.soft_dirty,
                                entry.file_page,
                                entry.swapped,
                                entry.present,
                                lib_name
                            );
                        }
                    }
                }
                buffer[y] = '\n';
            }
        }
    }
    close(maps_fd);
    close(pagemap_fd);

}



