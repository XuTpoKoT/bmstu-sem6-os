void printCMDLINE()
{
    char pathToOpen[
# 30 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c" 3 4
                   4096
# 30 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c"
                           ];
    snprintf(pathToOpen, 
# 31 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c" 3 4
                        4096
# 31 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c"
                                , "/proc/%d/cmdline", 9014);
    FILE *file = fopen(pathToOpen, "r");

    char buf[10000];
    int len = fread(buf, 1, 10000, file);
    buf[len - 1] = 0;

    fprintf(f, "\nCMDLINE CONTENT (полная командная строка процесса, если он не зомби):\n");
    fprintf(f, "%s\n", buf);

    fclose(file);
}

void printENVIRON()
{

    char pathToOpen[
# 47 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c" 3 4
                   4096
# 47 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c"
                           ];
    snprintf(pathToOpen, 
# 48 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c" 3 4
                        4096
# 48 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c"
                                , "/proc/%d/environ", 9014);
    FILE *file = fopen(pathToOpen, "r");

    int len;
    char buf[10000];
    fprintf(f, "\nENVIRON (файл окружения, исходная среда, которая была установлена при запуске выполняемой программы):\n");
    while ((len = fread(buf, 1, 10000, file)) > 0)
    {
        for (int i = 0; i < len; i++)
            if (!buf[i])
                buf[i] = '\n';
        buf[len - 1] = '\n';
        fprintf(f, "%s", buf);
    }

    fclose(file);
}

void printFD()
{
    char pathToOpen[
# 68 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c" 3 4
                   4096
# 68 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c"
                           ];
    snprintf(pathToOpen, 
# 69 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c" 3 4
                        4096
# 69 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c"
                                , "/proc/%d/fd/", 9014);
    DIR *dir = opendir(pathToOpen);


    fprintf(f, "\nFD(поддиректория, которая содержит одну запись (символическая ссылка на файл) для файла, открытого "
           "процессом):\n");

    struct dirent *readDir;
    char string[
# 77 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c" 3 4
               4096
# 77 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c"
                       ];
    char path[10000] = {'\0'};
    while ((readDir = readdir(dir)) != 
# 79 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c" 3 4
                                      ((void *)0)
# 79 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c"
                                          )
    {
        if ((strcmp(readDir->d_name, ".") != 0) && (strcmp(readDir->d_name, "..") != 0))
        {
            sprintf(path, "%s%s", pathToOpen, readDir->d_name);
            readlink(path, string, 
# 84 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c" 3 4
                                  4096
# 84 "/home/dmitriy/bmstu/SEM_6/OS/bmstu-sem6-os/lab_04/main2.c"
                                          );
            fprintf(f, "{%s} -- %s\n", readDir->d_name, string);
        }
    }

    closedir(dir);
}

